<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xuan.mapper.EmpLogMapper">
    <!-- 插入员工操作日志 -->
    <insert id="insert" parameterType="com.xuan.pojo.EmpLog">
        insert into emp_log (operate_time, info) 
        values (#{operateTime}, #{info})
    </insert>
</mapper>